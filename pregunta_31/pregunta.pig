data = LOAD 'data.csv' USING PigStorage(',') AS 
        ( number:int,
          nombre:charArray,
          apellido:charArray,
          fecha:charArray,
          color:charArray,
          end_number:int);

data = FOREACH data GENERATE SUBSTRING(fecha,0,4) as ano;
data_group = GROUP data BY ano;
data2 = FOREACH data_group GENERATE group, COUNT($1);

STORE data2 INTO 'output' USING PigStorage(',');